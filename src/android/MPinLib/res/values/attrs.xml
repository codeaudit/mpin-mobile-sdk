<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SelectionCircles">
        <attr name="circleDiameter" format="dimension|reference" />
        <attr name="spaceBetweenCircles" format="dimension|reference" />
        <attr name="selectedColor" format="color|reference" />
        <attr name="defaultColor" format="color|reference" />
        <attr name="circlesCount" format="integer" />
        <attr name="selectedStyle" format="enum">
            <enum name="filled" value="0" />
            <enum name="hollow" value="1" />
        </attr>
        <attr name="defaultStyle" format="enum">
            <enum name="filled" value="0" />
            <enum name="hollow" value="1" />
        </attr>
    </declare-styleable>

</resources>